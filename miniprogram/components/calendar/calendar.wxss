.not-month{
  color:#D0D4D6
}
.is-month{
  color: #657178;
}
.calender-header {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #032926;
}

.calender-header>image {
  width: 32rpx;
  height: 32rpx;
}
.calender-year-month{
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 550;
  color: #032926;
  white-space: nowrap;
}
.right-btn-groups {
  display: flex;
  position: absolute;
  right: 0;
  color: #B8B8B8;
}
.is-week-month{
  
}

.calender-main>.weeks {
  /* 字体颜色 */

  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  height: 64rpx;
  background: #6A829B;
  border-radius: 28rpx;
  display: flex;
}

.weeks>text {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 1;
}
.week-box{
  /* width: 100%; */
  /* overflow-y:auto;
  height:150rpx;
  display: flex;
   */
}
.scroll-view-item{
  /* width: 100%; */
  height: 118rpx;
  display: flex;
  white-space: nowrap;
}
.week-item{
  /* height: 190rpx; */
  height: 100%;
  width: 98rpx;
  display: inline-block;
  text-align: center;
  position: relative;
  color: #EF713B;
font-size: 24rpx;
}
.week-bottom-inline{
  width: 50%;
  height: 6rpx;
  background: red;
  border-radius: 3rpx;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}
.week-day-style{
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  justify-content: center;
  /* height:90%; */
}
.isweekday{
   background: linear-gradient(270deg, #EF753C 0%, #F13232 100%);
  color: #FFFFFF; 
}

.isweekday-checkout{
  border: 1px solid #EF6D3B;
}
.cell {
  display: flex;
}
.day-box{
  font-size: 22rpx;
  font-family: Avenir-Heavy, Avenir;
  font-weight: 800;
  flex: 1;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  /* height: 80rpx; */
  height: 90rpx;

}
.day {
  /* 字体颜色 */
  font-size: 26rpx;
  font-family: Avenir-Heavy, Avenir;
  font-weight: 800;
  height: 90rpx;
  flex: 1;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}
.istoday,.check-day{
  display: inline-block;
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  
}
.istoday{
  background: linear-gradient(270deg, #EF753C 0%, #F13232 100%);
  font-size: 28rpx;
font-family: Avenir-Heavy, Avenir;
font-weight: 800;
color: #FFFFFF;
}
.check-day{
  background: #FFFFFF;
  border: 2rpx solid #EF6D3B;
font-size: 26rpx;
font-family: Avenir-Heavy, Avenir;
font-weight: 800;
color: #EF6D3B;
  }
.bottom-inline{
  width: 50%;
  height: 6rpx;
  background: #EF713B;
  border-radius: 3rpx;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}
.test{
  color: pink;
}
