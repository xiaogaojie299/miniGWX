.not-month{
  color:#D0D4D6
}
.is-month{
  color: #657178;
}
.calender-header {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #032926;
}

.calender-header>image {
  width: 32rpx;
  height: 32rpx;
}
.calender-year-month{
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 550;
  color: #032926;
  white-space: nowrap;
}
.right-btn-groups {
  display: flex;
  position: absolute;
  right: 0;
}

.calender-main>.weeks {
  /* 字体颜色 */

  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  height: 64rpx;
  background: #6A829B;
  border-radius: 28rpx;
  display: flex;
}

.weeks>text {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 1;
}

.cell {
  display: flex;
}
.day-box{
  font-size: 22rpx;
  font-family: Avenir-Heavy, Avenir;
  font-weight: 800;
  flex: 1;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  /* height: 80rpx; */
  height: 90rpx;
}
.day {
  /* 字体颜色 */
  font-size: 26rpx;
  font-family: Avenir-Heavy, Avenir;
  font-weight: 800;
  height: 90rpx;
  flex: 1;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}
.istoday,.check-day{
  display: inline-block;
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  
}
.istoday{
  background: linear-gradient(270deg, #EF753C 0%, #F13232 100%);
  font-size: 28rpx;
font-family: Avenir-Heavy, Avenir;
font-weight: 800;
color: #FFFFFF;
}
.check-day{
  background: #FFFFFF;
  border: 2rpx solid #EF6D3B;
font-size: 26rpx;
font-family: Avenir-Heavy, Avenir;
font-weight: 800;
color: #EF6D3B;
  }
.bottom-inline{
  width: 50%;
  height: 6rpx;
  background: #EF713B;
  border-radius: 3rpx;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}
.isToday{
background: linear-gradient(270deg, #EF753C 0%, #F13232 100%);
border-radius: 50%;
}
.test{
  color: pink;
}
