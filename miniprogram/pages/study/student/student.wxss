.w-200{
  width: 200rpx;
}
.header{
  display: flex;
  justify-content: space-between;
}
.header>.left-cont,.right-cont{
  height: 60rpx;
}
.left-cont{
  width: 50%;
font-size: 26rpx;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #333333;
}
.left-cont .class-name{
  border: 1px solid red;
  color: red !important;
}
.right-cont{
  width: 68%;
}
.left-cont>text{
    white-space: nowrap;
}
.left-cont>image{
  margin-left: 12rpx;
  width: 16rpx;
  height: 8rpx;
}
.right-cont>image{
  width: 28rpx;
  height: 26rpx;
}

/*main */
.stu-row>.stu_icon{
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
}
.student-detail{
}
.student-detail>image{
  width: 28rpx;
  height: 28rpx;
}
.stu-font{
font-size: 26rpx;
font-family: PingFangSC-Regular, PingFang SC;
font-weight: 400;
color: #666666;
}

.top-input {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.left-cont {
  width: 100%;
  width: 100%;
  height: 60rpx;
  position: relative;
}

.left-cont>input {
  width: 380rpx;
  height: 100%;
  border-radius: 32rpx;
  padding:0 62rpx;
  background: #F5F5F5;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}

.left-cont>image {
  position: absolute;
  width: 28rpx;
  height: 26rpx;
  top: 50%;
  left: 24rpx;
  transform: translateY(-50%);
}
.right-text{
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  white-space: nowrap;
  margin-left:14rpx;
}